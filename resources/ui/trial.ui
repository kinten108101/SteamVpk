<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <!-- interface-name trial.ui -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkWindow">
    <property name="title">Hi!</property>
    <property name="titlebar">head</property>
  </object>
  <object class="GtkHeaderBar" id="head">
    <property name="title-widget">xlabel</property>
    <child>
      <object class="GtkBox"/>
    </child>
    <child type="end">
      <object class="GtkButton"/>
    </child>
    <child>
      <object class="GtkLabel" id="xlabel">
        <property name="label">Hi!</property>
      </object>
    </child>
  </object>
</interface>
